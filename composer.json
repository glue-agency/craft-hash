    "autoload": {
        "psr-4": {
          "gluecrafthash\\crafthash\\": "src/"
        }
    },
    "extra": {
        "name": "Craft Hash",
        "handle": "craft-hash",
        "hasCpSettings": false,
        "hasCpSection": false,
