        "issues": "https://github.com/glue-agency/craft-hash/issues"
    },
    "license": "GPL-3.0-only",
    "authors": [
        {
            "name": "Glue Agency",
            "homepage": "https://glue.be"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0-RC1"
